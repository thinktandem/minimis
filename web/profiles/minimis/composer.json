{
    "name": "drupal/minimis",
    "description": "Minimis Distribution",
    "type": "drupal-profile",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "John Ouellet",
            "email": "john@thinktandem.io",
            "homepage": "https://thinktandem.io",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "installer-paths": {
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/profiles/minimis/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/minimis/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/profiles/minimis/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/minimis/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches-file": "composer.patches.json"
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/adminimal_theme": "^1.3",
        "drupal/adminimal_admin_toolbar": "^1.5",
        "drupal/admin_toolbar": "^1.23",
        "drupal/bootstrap_barrio": "^4.6",
        "drupal/bootstrap_library": "^1.9",
        "drupal/config_readonly": "^1.0",
        "drupal/config_split": "^1.2",
        "drupal/ctools": "^3.0",
        "drupal/dynamic_layouts": "^1.0",
        "drupal/linkit": "^4.3",
        "drupal/metatag": "^1.3",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/redirect": "^1.0",
        "drupal/schema_metatag": "^1.0",
        "drupal/token": "^1.0",
        "drupal/twitter_block": "^2.1"
    }
}
